[Desktop Entry]
Type=Service
MimeType=video/*;audio/*;
Actions=aac2flacReplace;aac2flacMkv;toaacReplace;toaacMp4;toDavinciResolveReplace;toDavinciResolveMov
X-KDE-Submenu=Davinci Resolve Conversions
X-KDE-Priority=TopLevel
Name=Davinci Resolve Conversions

[Desktop Action aac2flacReplace]
Name=Convert AAC audio to FLAC (replace)
Icon=audio-x-flac
Exec=/path/to/scripts/aac2flac_replace %F

[Desktop Action aac2flacMkv]
Name=Convert AAC audio to FLAC (.mkv)
Icon=audio-x-flac
Exec=/path/to/scripts/aac2flac_mkv %F

[Desktop Action toaacReplace]
Name=Convert audio to AAC (replace original)
Icon=audio-x-aac
Exec=/path/to/scripts/toaac_replace %F

[Desktop Action toaacMp4]
Name=Convert audio to AAC (smart container)
Icon=audio-x-aac
Exec=/path/to/scripts/toaac_mp4 %F

[Desktop Action toDavinciResolveReplace]
Name=Convert to DNxHD/DNxHR (replace)
Icon=video-x-generic
Exec=/path/to/scripts/to_davinci_resolve_replace %F

[Desktop Action toDavinciResolveMov]
Name=Convert to DNxHD/DNxHR (.mov)
Icon=video-x-generic
Exec=/path/to/scripts/to_davinci_resolve_mov %F